@{
    ViewBag.Title = "Phòng máy";
    ViewData["icon"] = "fa fa-desktop fa-fw";
    ViewData["sub"] = "Danh sách";
    ViewData["url"] = "/pcroom";
}
<div class="table-responsive">
    <table class="table table-hover table-bordered table-striped">
        <thead>
            <tr class="text-info">
                <th>#</th>
                <th>Tên phòng</th>
                <th>Vị trí</th>
                <th>Số máy</th>
                <th>Hoạt động</th>
                <th>Màn hình</th>
                <th>Mainboard</th>
                <th>CPU</th>
                <th>RAM</th>
                <th>VGA</th>
                <th>SSD</th>
                <th>HDD</th>
                <th>Bàn phím</th>
                <th>Chuột</th>
                <th>Tai nghe</th>
                <th>Loa</th>
                <th>PSU</th>
                <th>Trạng thái</th>
                <th>Ghi chú</th>
                <th></th>
            </tr>
        </thead>
        <tbody id="tblRooms">
            
        </tbody>
    </table>
</div>

@section scripts{
    <script src="~/Assets/defined/pcroom.js"></script>
}